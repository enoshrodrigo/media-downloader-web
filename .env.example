# Example .env file for MediaFlow

# Database configuration
DB_HOST=localhost
DB_USER=your_username
DB_PASS=your_password
DB_NAME=media_downloader

# Directory paths
MEDIA_VIDEOS_DIR=media/videos/
MEDIA_MUSIC_DIR=media/music/

# Base URL
BASE_URL=http://localhost/media-downloader/

# Other constants
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=video/mp4,audio/mpeg,image/jpeg,image/png
